{
  "name": "go-dev",
  "image": "ghcr.io/willfantom/devcontainer-go:latest",
  "workspaceMount": "source=${localWorkspaceFolder}/,target=/workspace,type=bind,consistency=cached",
  "workspaceFolder": "/workspace",
  "features": {
    "ghcr.io/willfantom/features/dotfiles:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go"
      ],
      "settings": {}
    }
  }
}